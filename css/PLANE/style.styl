$bg_color = #ffd200
*
    margin 0
    padding 0

html,body
    width 100%
    height 100%

.plane-page
    width 100%
    height 100%
    background-color $bg_color
    position relative
    .plane-wrapper
        position absolute
        left 20%
        top 40%
        animation undulate 8s linear infinite alternate
        .plane
            width 100%
        .rotate-container
            position absolute
            top 51.7003%
            left 13.1%
            width 27.424%
            height 14.447%
            transform-origin center center
            animation rotating .1s linear infinite reverse
            perspective: 2500px
            .propeller
                width 100%
                transform rotateY(-28deg)
@keyframes rotating {
    0%{
         transform rotateZ(0deg)   rotateY(-28deg)
       }
    50%{
        transform rotateZ(180deg) rotateY(-28deg)
        }
    100%{
        transform rotateZ(360deg) rotateZ(-28deg)
        }
}
@keyframes undulate{
    0%,50%,100%{
        transform translate3d(0,0,0)
        }
    25%{
        transform translate3d(-6px,-18px,-10px)
        }
    75%{
        transform translate3d(6px,18px,10px)
        }
    
    }